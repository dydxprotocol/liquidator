  "bugs": {
    "url": "https://github.com/dydxprotocol/liquidator/issues"
  },
  "homepage": "https://github.com/dydxprotocol/liquidator#readme",
  "dependencies": {
    "@dydxprotocol/solo": "^0.15.4",
    "bignumber.js": "^8.1.1",
    "dotenv-flow": "^0.4.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "luxon": "^1.21.2",
    "request-promise-native": "^1.0.8",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
